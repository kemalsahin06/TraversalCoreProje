@model List<TraversalCoreProje.Areas.Admin.Models.ApiMovieViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>api Imdb Top 100 flim listesi</h1>

<br />

<table class="table table-striped">

    <tr>
        <td>#</td>
        <td>Flim Adı</td>
        <td>Imdp Puanı</td>
        <td>Yapım Yılı</td>
        <td>Fragman</td>
        
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.rank</td>
            <td>@item.title</td>
            <td>@item.rating</td>
            <td>@item.year</td>
            <td><a href="#" class="btn btn-outline-dark">Fragman</a></td>
        </tr>
    }



</table>