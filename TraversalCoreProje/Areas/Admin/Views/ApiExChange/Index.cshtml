@model IEnumerable<BookingExChangeViewModel_2.Exchange_Rates>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h1>Booking Kur Bilgileri</h1>

<table class="table table-striped">
    <tr>
        <td>Kurs Adı</td>
        <td>Kurs Oranı</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>
        </tr>
    }
</table>